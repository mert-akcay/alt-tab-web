@using _02_MVC_Example.Models
@model User

<div class="form-box">
    <table>
        <thead>
            <tr>
                <th>Ad</th>
                <th>Soyad</th>
                <th>E-posta</th>
                <th>Doğum Tarihi</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.Ad</td>
                <td>@Model.Soyad</td>
                <td>@Model.Email</td>
                <td>@Model.DogumTarihi</td>
            </tr>
        </tbody>
    </table>
</div>